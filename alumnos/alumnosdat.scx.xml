		<protected/>
		<methods>PROCEDURE Click
*
*	btnGuardar()
*

with thisform
	if(.nModo &lt;= 2)
		if .nModo == 1
			if empty(.txtClave.Value)
				Alerta('No se puede omitir la clave del alumno')
				.Clave.SetFocus()
				return .f.
			endif
			
			if seek(PadL(Allt(.Clave.Value), 5), 'Alumnos')
				Alerta('Ya existe un alumno con la misma clave')
				.Clave.SetFocus()
				return .f.
			endif
	endif
			if empty(.txtNombre.Value)
				Alerta('No se puede omitir el nombre')
				.txtNombre.SetFocus()
				return .f.
			endif
		endif
		
		do case
			case nModo == 1
				select Alumnos
				Append blank
				.SaveInfo()
				.Release
			case nModo == 2
				.SaveInfo()
				.Release
			case nModo == 3
				if GetNY('Â¿Desea eliminar el alumno?')
					select Alumnos
					Delete
				endif
				.Release
		endcase
endwith
ENDPROC
