		<methods>PROCEDURE Init
*
*	init()
*

LParameter nModo, nRec

*
* nModo
*	1 = Agregar
*	2 = Modificar
*	3 = Eliminar
*

if not OpenDbf('','')
	retur .f.
endif

with thisform
	if PCount() == 0
		nModo = 1
	endif
	
	.nModo = nModo
	
	if(.nModo == 2 OR .nModo ==3)
		if(nRec &lt;= 0 OR nRec &gt; Reccount())
			Alerta('Registro no valido')
			return .f.
		endif
		goto nRec
	endif
	
	do case
		case .nModo == 1
			.Caption = 'Agregar Alumno'
			.btnGrabar.Caption = '\&lt;Agregar'
			Select Alumnos
			goto bott
			.Clave.Value = PadL(Allt(SigDoc(ID)),5)
			
		case .nModo == 2
			.Caption = 'Modificar Alumno'
			.btnGrabar.Caption = '\&lt;Modificar'
			.Clave.Enabled = .f.
			.CargarInfo()
			
		case nModo == 3
			.Caption = 'Eliminar Alumno'
			.btnGrabar.Caption = '\&lt;Eliminar'
			.CargarInfo()
			.SetAll('enabled',.f.)
			.btnGrabar.enabled = .t.
			.btnCerrar.enabled = .t.
		endcase
endwith
ENDPROC
PROCEDURE cargarinfo
*
*	CargarInfo()
*

* Params
*	Clave = Identificador de la tabla
*	Nombre = Nombre del alumno
*	Apellidos = Apelldios del alumno
*	Grado = maximo un caracter ejemplo: 1, 2 o 3
*	Grupo = maximo un caracter ejemplo: A, B o C
*	Matricula = numero de expediente del alumno
*	FechaNac = Fecha de nacimiento
*

with thisform
	.txtClave.Value = Alumnos.ID
	.txtNombre = Alumnos.NOMBRE
	.txtApellidos = Alumnos.APELLIDOS
	.txtGrado = Alumnos.GRADO
	.txtGrupo = Alumnos.GRUPO
	.txtMatricula = Alumnos.MATRICULA
	.txtFechaNac = Alumnos.FECHANAC
endwith

ENDPROC
PROCEDURE saveinfo
*
*	SaveInfo()
*	

with thisform
	select Alumnos
	replace ID 			with .txtClave.Value,;
			NOMBRE 		with Allt(.txtNomre.Value),;
			APELLIDOS 	with Allt(.txtApellidos.Value),;
			GRADO 		with Allt(.txtGrado.Value),;
			GRUPO 		with Allt(.txtGrupo.Value),;
			MATRICULA 	with Allt(.txtMatricula.Value),;
			FECHANAC 	with Allt(.txtFechaNac.Value)
endwith

ENDPROC
